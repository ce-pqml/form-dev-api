{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "start": {
      "type": "datetime",
      "required": true
    },
    "end": {
      "type": "datetime",
      "required": true
    },
    "signatures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::signature.signature",
      "mappedBy": "course"
    },
    "speakers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::speaker.speaker",
      "inversedBy": "courses"
    },
    "course_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-comment.course-comment",
      "mappedBy": "course"
    },
    "key": {
      "type": "uid"
    },
    "signature": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false
    },
    "signature_date": {
      "type": "datetime"
    }
  }
}
